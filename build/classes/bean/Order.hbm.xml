<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="bean">
	<class name="Order" table="ORDERU">
		<id name="id" column="id">
			<generator class="native">
			</generator>
		</id>
		<property name="uid" column="uid" type="java.lang.Integer"/>
		<property name="pid" column="pid" type="java.lang.Integer"/>
	    <property name="orderid" column="order_id" type="java.lang.String"/>
		<property name="information" column="information" type="java.lang.String"/>
		<property name="time" column="time" type="java.lang.String"/>
			<property name="number" column="number" type="java.lang.Integer"/>
		
		
		
		
	</class>
	
</hibernate-mapping>